<script type="text/javascript">

// This should be an EXCELLENT feature!!!
function maskSSN(){
  var fullSSN = $("td.ssn").html().replace(/\d{3}.*?\d{2}/, "*****");
  $("td.ssn").html(fullSSN);

}

function makeActive(){
  $('li[id="employee_info"]').addClass('active');
};

$(document).ready(function () {
   maskSSN(),
   makeActive()
});

</script>
