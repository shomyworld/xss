<script type="text/javascript">

function makeActive(){
  $('li[id="home"]').addClass('active');
};

$("#change_to_bar_graph").click(function(event) {
	event.preventDefault();
	$("#charts_body").empty()
	$("#charts_body").load("/dashboard/change_graph?graph=bar_graph");
	
})

$("#change_to_pie_charts").click(function(event) {
	event.preventDefault();
	$("#charts_body").empty()
	$("#charts_body").load("/dashboard/change_graph?graph=pie_charts");
})

$(document).ready(
  makeActive,
  $("#charts_body").load("/dashboard/change_graph?graph=pie_charts")
);
</script>
