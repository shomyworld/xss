    function searchByAddress(obj) {
        if(obj.free_word.value == "フリーワードを入力" || obj.free_word.value == "Search Word") {
            obj.free_word.value = "";
        }

        obj.search_by_address_flg.value = 1;

        obj.submit();
    }

    function MainFunction(b) {
        if (document.formMain.pref_code.selectedIndex == 0) {
            document.formMain.city.length = 1;
            document.formMain.city.selectedIndex = 0;
        } else {
            if (b) {
                document.formMain.city.length = 1;
                document.formMain.city.selectedIndex = 0;
            }
            var city = cities2[document.formMain.pref_code.options[document.formMain.pref_code.selectedIndex].value];
            document.formMain.city.length = city.length + 1;
            document.formMain.city.options[0].value = '';
            document.formMain.city.options[0].text = '市区郡で絞り込む';
            $j1111('#cityDiv').children('.js-label').text('市区郡で絞り込む');
            for (var i = 0; i < city.length; i++) {
                document.formMain.city.options[i + 1].value = city[i];
                document.formMain.city.options[i + 1].text = city[i];

                // 選択済みにする
                if (city[i] == "") {
                    document.formMain.city.options[i + 1].selected = true;
                }
            }

            // 選択した都道府県に市区町村が有るか無いかで、表示非表示を切り替える
            if (cities2[document.formMain.pref_code.options[document.formMain.pref_code.selectedIndex].value].length > 0) {
                $j1111('#cityDiv').show();
            } else {
                $j1111('#cityDiv').hide();
            }
        }
    }

